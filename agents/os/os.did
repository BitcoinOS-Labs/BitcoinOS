
type WalletOwnerResult = variant {
    Ok: WalletOwner;
    Err: Error;
};

type WalletOwner = record {
    canister_id: principal;
    owner: principal;
    created_at: nat64;
};

type Error = variant {
    AlreadyExists;
    WriteError;
    Unknown;
};

service : {
    "create_wallet": () -> (WalletOwnerResult);

    "count_wallet": () -> (nat64) query;
};

